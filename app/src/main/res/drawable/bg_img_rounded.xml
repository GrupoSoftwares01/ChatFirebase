<?xml version="1.0" encoding="utf-8"?>

<!-- Aqui é o original do primeiro arquivo "selector"
<selector xmlns:android="http://schemas.android.com/apk/res/android">
</selector>
-->

<!-- Aqui abaixo o "selector" será trocado pro "shape" -->
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!--Aqui com a tag "<solid>" podemos construir cores sólidas no objeto, no caso "holo_green_dark" -->
    <!--
    Aqui com a tag "<corners>" podemos construir bordas arredondadas no objeto, no caso a imagem do botão
    -->
    <solid android:color="@android:color/holo_green_dark" />
    <corners android:radius="130dp" />

</shape>